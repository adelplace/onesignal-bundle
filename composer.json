{
  "name": "birkof/onesignal-bundle",
  "description": "Symfony integration of the norkunas/onesignal-php-api library",
  "type": "symfony-bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "birkof",
      "email": "birkof@gmail.com"
    }
  ],
  "require": {
    "php": "^5.3|^7.1",
    "guzzlehttp/guzzle": "^6.3",
    "norkunas/onesignal-php-api": "^2.1",
    "nyholm/psr7": "^1.3",
    "php-http/guzzle6-adapter": "^1.1",
    "symfony/framework-bundle": "^2.8|^3.0|^4.0",
    "symfony/http-client": "^5.1"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Adelplace\\OneSignalBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "vendor/norkunas/onesignal-php-api/src"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
